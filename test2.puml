!includeurl https://raw.githubusercontent.com/ArmanAvanesyan/test/main/DFD.puml

' Start & End of DFD

!quoted procedure startDfd()
digraph dfd2 {
    rankdir="LR"
    node[shape=record]
!endprocedure

!quoted procedure endDfd()
}
!endprocedure

' Elements

!unquoted procedure External($alias, $label)
    $alias [label="$label" shape=box];
!endprocedure

!unquoted procedure Process($alias, $label)
    $alias [label="{<f0> 0 |<f1> $label\n\n\n}" shape=Mrecord];
!endprocedure
