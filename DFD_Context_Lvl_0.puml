!includeurl https://raw.githubusercontent.com/ArmanAvanesyan/test/main/DFD.puml
!$level="Context Diagram (DFD Level 0)"

!$EXTERNAL_SHAPE = "box"
!$EXTERNAL_FILLCOLOR = "lightsteelblue1"
!$EXTERNAL_WIDTH = 1
!$EXTERNAL_HEIGHT = 2

!$PROCESS_SHAPE = "Mrecord"
!$PROCESS_FILLCOLOR = "lightsteelblue1"
!$PROCESS_WIDTH = 2
!$PROCESS_ HEIGHT = 1

!$DEFAULT_STYLE = "filled"
!$DEFAULT_FONTCOLOR = "#000000"
!$DEFAULT_FONTSIZE = 12
!$DEFAULT_BORDERCOLOR = "grey"

!unquoted procedure External($alias, $label)
    Component($alias, $label, $EXTERNAL_SHAPE, $EXTERNAL_FILLCOLOR, DEFAULT_STYLE, $DEFAULT_FONTCOLOR, $DEFAULT_FONTSIZE, $DEFAULT_BORDERCOLOR, $EXTERNAL_WIDTH, $EXTERNAL_HEIGHT)
!endprocedure

!unquoted procedure Process($alias, $label)
    Component($alias, $label, $PROCESS_SHAPE, $PROCESS_FILLCOLOR, DEFAULT_STYLE, $DEFAULT_FONTCOLOR, $DEFAULT_FONTSIZE, $DEFAULT_BORDERCOLOR, $PROCESS_WIDTH, $PROCESS_ HEIGHT)
!endprocedure
