!includeurl https://raw.githubusercontent.com/ArmanAvanesyan/test/main/DFD.puml
!$level="Context Diagram (DFD Level 0)"

!$color_external = "lightcoral"
!$color_process = "lightblue"
!$default_fontsize = 12
!$default_width = 2
!$default_height = 2
!$default_fontcolor = "#000000"
!$default_bordercolor = "grey"

!unquoted procedure External($alias, $label)
    Component($alias, $label, "box", $color_external, $default_fontsize, 1, $default_height)
!endprocedure

!unquoted procedure Process($alias, $label)
    Component($alias, "0 | $label\n\n", "Mrecord", $color_process, $default_fontsize, $default_width, $default_height)
!endprocedure
