!procedure External($alias, $label)
    $alias [label="$label", shape=box, fillcolor=lightblue, style=filled, fontcolor=black, fontsize=12, color=black, width=2];
!endprocedure

!procedure Process($alias, $id, $name)
    $alias [label="{<f0> $id |<f1> $name\n\n\n}", shape=Mrecord, style=bold];
!endprocedure

!procedure Store($alias, $id, $name)
    $alias [label="<f0> $id   |<f1> $name", fillcolor=lightblue, style=filled];
!endprocedure

!procedure Flow_R($from, $to, $label = "")
    !if ($label != "")
        $from -> $to [label="$label" dir=forward fontcolor=black fontsize=10 color=black width=1];
    !else
        $from -> $to [dir=forward fontcolor=black fontsize=10 color=black width=1];
    !endif
!endprocedure

!procedure Flow_L($from, $to, $label = "")
    !if ($label != "")
        $from -> $to [label="$label" dir=back fontcolor=black fontsize=10 color=black width=1];
    !else
        $from -> $to [dir=back fontcolor=black fontsize=10 color=black width=1];
    !endif
!endprocedure

!procedure BiFlow($from, $to, $label = "")
    !if ($label != "")
        $from -> $to [label="$label" dir=both fontcolor=black fontsize=10 color=black width=1];
    !else
        $from -> $to [label="$label" dir=both fontcolor=black fontsize=10 color=black width=1];
    !endif
!endprocedure
